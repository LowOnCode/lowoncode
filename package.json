  "bin": {
    "lowoncode": "./bin/lowoncode"
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "ajv": "^6.10.2",
    "axios": "^0.19.0",
    "cli-color": "^1.4.0",
    "cli-loader": "^1.1.1",
    "cli-table": "^0.3.1",
    "commander": "^4.0.1",
    "debug": "^4.1.1",
    "dotenv": "^8.1.0",
    "eventemitter2": "^5.0.1",
    "glob": "^7.1.6",
    "koa": "^2.8.2",
    "koa-body": "^4.1.1",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa2-cors": "^2.0.6",
    "node-fetch": "^2.6.0",
    "ws": "^7.1.2"
  }
